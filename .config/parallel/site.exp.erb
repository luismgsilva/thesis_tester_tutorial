set rootme "<%=workspace %>"
set srcdir "<%=gcc_dir %>/gcc"
set host_triplet x86_64-pc-linux-gnu
set build_triplet x86_64-pc-linux-gnu
set target_triplet <%=target_triplet %>
set target_alias <%=target_alias %>
set libiconv ""
set CFLAGS ""
set CXXFLAGS ""
set HOSTCC "gcc"
set HOSTCXX "g++"
set HOSTCFLAGS "<%=hostcflags %>"
set HOSTCXXFLAGS "<%=hostcxxflags %>"
set TEST_ALWAYS_FLAGS ""
set TEST_GCC_EXC_PREFIX "<%=toolchain_install_dir %>/lib/gcc/"
set TESTING_IN_BUILD_TREE 1
set HAVE_LIBSTDCXX_V3 1
set ENABLE_PLUGIN 1
set PLUGINCC "g++"
set PLUGINCFLAGS "<%=plugincflags %>"
set GMPINC ""
set tmpdir ${rootme}/testsuite
set srcdir "${srcdir}/testsuite"
